#!/bin/sh

set VEHICLE_TYPE fw

if [ $AUTOCNF = yes -o $NEW_DEFAULTS = yes ]
then
#
# Default parameters for FW
#
	param set RTL_RETURN_ALT 100
	param set RTL_DESCEND_ALT 100
	param set RTL_LAND_DELAY -1

	param set NAV_ACC_RAD 50

	# Mag calibration setting
	# do two-side calibration
	param set CAL_MAG_SIDES 34

	# Logger - prefer the new logging system
	param set SYS_LOGGER 1
fi

# This is the gimbal pass mixer
set MIXER_AUX pass
set PWM_AUX_RATE 50
set PWM_AUX_OUT 1234
set PWM_AUX_DISARMED 1500
set PWM_AUX_MIN 1000
set PWM_AUX_MAX 2000
